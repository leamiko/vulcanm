{
  "compilerOptions": {
    "lib": [
      "dom", "es5", "es2015", "es2015.promise"
    ],
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "typeRoots": [
      "node_modules/@types"
    ],
    "paths": {
      "lodash/*": [
        "node_modules/@types/lodash-es/*"
      ]
    }
  }

}
